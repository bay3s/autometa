{
  "algo": "auto_dr",
  "env_name": "PointRobotNavigation-v1",
  "random_seed": 6785,
  "env_configs": {
    "episode_length": 100
  },
  "actor_lr": 0.0003,
  "critic_lr": 0.0001,
  "use_linear_lr_decay": false,
  "optimizer_eps": 0.0000000001,
  "max_grad_norm": 0.5,
  "cuda_deterministic": false,
  "use_cuda": true,
  "policy_iterations": 250,
  "meta_episode_length": 100,
  "meta_episodes_per_epoch": 2500,
  "num_processes": 20,
  "discount_gamma": 0.99,
  "adr_evaluation_probability": 0.5,
  "adr_performance_buffer_size": 50,
  "adr_performance_threshold_lower": -250,
  "adr_performance_threshold_upper": -25,
  "adr_delta": 0.03,
  "ppo_opt_epochs": 10,
  "ppo_num_minibatches": 50,
  "ppo_clip_param": 0.1,
  "ppo_entropy_coef": 0.01,
  "ppo_value_loss_coef": 0.5,
  "use_gae": true,
  "gae_lambda": 0.99,
  "checkpoint_interval": 1,
  "checkpoint_all": false
}
